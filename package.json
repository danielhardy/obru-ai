{
  "name": "obru-ai",
  "version": "0.1.3",
  "description": "TypeScript toolkit for creating AI agents with custom tools and workflows",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:deno": "deno test --allow-net --allow-env",
    "lint": "eslint src/**/*.ts",
    "lint:deno": "deno lint src/",
    "fmt:deno": "deno fmt src/",
    "check:deno": "deno check src/index.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "ai",
    "agent",
    "openai",
    "llm",
    "tools",
    "workflow",
    "deno",
    "typescript"
  ],
  "author": "Daniel Hardy",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "deno.json",
    "jsr.json"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielhardy/obru-ai.git"
  },
  "bugs": {
    "url": "https://github.com/danielhardy/obru-ai/issues"
  },
  "homepage": "https://github.com/danielhardy/obru-ai#readme"
}
